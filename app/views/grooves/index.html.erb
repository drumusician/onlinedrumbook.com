<h1>Grooves</h1>
<%= link_to 'Add Groove', new_groove_path %>
<p><% @grooves.each do |g| %>
  <ul>
    <li>id: <%= g.id %></li>
    <li>image: <%= image_tag g.image.url(:thumb) %></li>
    <li>length: <%= g.length %></li>
    <li>audio: <%= g.audio %></li>
    <li>video: <%= g.video %></li>
   </ul>
<% end %></p>
